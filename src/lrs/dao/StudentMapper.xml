<!DOCTYPE mapper
        PUBLIC "-//mybatis.org/DTD Mapper 3.0"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="lrs.dao.StudentMapper">
    <select id="queryStudentById" parameterType="int" resultType="lrs.entity.Student">
